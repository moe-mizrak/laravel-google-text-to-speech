{
    "name": "moe-mizrak/laravel-google-text-to-speech",
    "description": "Laravel package for integrating Gemini Text-to-Speech API and Google Cloud Text-to-Speech API",
    "keywords": [
        "Moe Mizrak",
        "laravel",
        "laravel-google-text-to-speech",
        "php 8",
        "google-cloud",
        "text-to-speech",
        "gemini",
        "api",
        "ai-voice"
    ],
    "type": "package",
    "homepage": "https://github.com/moe-mizrak/laravel-google-text-to-speech",
    "license": "MIT",
    "authors": [
        {
            "name": "Moe Mizrak"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-data": "^4.17",
        "google/cloud-text-to-speech": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.6.1",
        "phpunit/phpunit": "^11.4",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
          "MoeMizrak\\LaravelGoogleTextToSpeech\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "MoeMizrak\\LaravelGoogleTextToSpeech\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "MoeMizrak\\LaravelGoogleTextToSpeech\\LaravelGoogleTextToSpeechServiceProvider"
          ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
